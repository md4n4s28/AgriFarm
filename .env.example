# AgriFarm Environment Variables - EXAMPLE FILE
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Model API Endpoints (Your team's backend)
AI_CROP_DETECTION_URL=http://your-backend-url/api/crop-detection
AI_PRICE_PREDICTION_URL=http://your-backend-url/api/price-prediction
AI_DISEASE_DETECTION_URL=http://your-backend-url/api/disease-detection

# External APIs
WEATHER_API_KEY=your_openweathermap_api_key
WEATHER_API_URL=https://api.openweathermap.org/data/2.5

# Groq AI
GROQ_API_KEY=your_groq_api_key_here

# Market Price APIs
AGMARKNET_API_URL=https://api.data.gov.in/resource/9ef84268-d588-465a-a308-a864a43d0070
ENAM_API_URL=https://enam.gov.in/web/api

# Satellite Imagery
GOOGLE_EARTH_ENGINE_KEY=your_gee_key
ISRO_BHUVAN_API_KEY=your_bhuvan_key

# IoT Configuration (MQTT for ESP32)
MQTT_BROKER_URL=broker.hivemq.com
MQTT_USERNAME=your_mqtt_username
MQTT_PASSWORD=your_mqtt_password


